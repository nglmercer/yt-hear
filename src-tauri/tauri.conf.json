{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "devPath": "http://localhost:1420",
    "distDir": "./dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "tatar",
    "version": "0.0.2"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.github.2gn",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": "default-src 'self' https://music.youtube.com https://*.googlevideo.com https://*.youtube.com https://*.google.com; script-src 'self' 'unsafe-inline' https://music.youtube.com https://*.youtube.com; style-src 'self' 'unsafe-inline' https://music.youtube.com https://*.youtube.com; img-src 'self' data: https://*.googleusercontent.com https://*.youtube.com https://*.google.com; media-src 'self' https://*.googlevideo.com https://*.youtube.com; connect-src 'self' https://music.youtube.com https://*.youtube.com https://*.google.com"
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "label": "settings",
        "fullscreen": false,
        "width": 800,
        "height": 600,
        "resizable": false,
        "title": "Settings",
        "visible": false
      },
      {
        "label": "main",
        "url": "https://music.youtube.com",
        "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36",
        "minWidth": 320.0,
        "minHeight": 320.0,
        "title": "tatar",
        "decorations": true,
        "fileDropEnabled": true
      }
    ]
  }
}